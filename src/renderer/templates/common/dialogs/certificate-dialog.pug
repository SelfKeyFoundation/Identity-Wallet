md-dialog.generic-info-container(aira-lable="{{title}}")
    .info-wrapper
        .info-title
            span {{ title }}
            span.close(ng-click="cancel($event)") x
        .info-body 
            div {{ text }}
            md-button.default(ng-click="accept($event)")
                span.primary accept
            md-button.default.cancel-btn(ng-click="cancel($event)")
                span.primary close