{
  "name": "selfkey-identity-wallet",
  "productName": "SelfKey Identity Wallet",
  "version": "0.0.1",
  "description": "The Official SelfKey Identity Wallet for Desktop",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SelfKeyFoundation/Identity-Wallet.git"
  },
  "keywords": [
    "selfkey",
    "identity",
    "wallet",
    "cryptocurrency",
    "ethereum",
    "erc20"
  ],
  "author": {
    "name": "SelfKey Foundation",
    "email": "info@selfkey.org",
    "url": "https://selfkey.org/"
  },
  "bugs": {
    "url": "https://github.com/SelfKeyFoundation/Identity-Wallet/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/MIT"
    }
  ],
  "main": "./app/main.js",
  "scripts": {
    "start": "gulp && ./node_modules/.bin/electron-forge start -- dev",
    "prod": "gulp && ./node_modules/.bin/electron-forge start -- prod",
    "package": "gulp && ./node_modules/.bin/electron-forge package",
    "make": "gulp && ./node_modules/.bin/electron-forge make",
    "package-win": "gulp && node_modules\\.bin\\electron-forge package --platform=win32 --arch=ia32",
    "make-win": "gulp && node_modules\\.bin\\electron-forge make --platform=win32 --arch=ia32",
    "start-win": "gulp && node_modules\\.bin\\electron-forge start -- dev",
    "full": "bash full.sh",
    "test": "node test/test.js",
    "lint": "eslint 'app/src/**/*.js'",
    "precommit": "lint-staged",
    "cz": "git-cz",
    "docker": "OSENV=docker docker-compose build && docker-compose up -d",
    "mig-new": "knex migrate:make",
    "mig-test": "node test/migrate.js"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@uirouter/angularjs": "1.0.15",
    "angular": "1.7.0",
    "angular-animate": "1.7.0",
    "angular-aria": "1.7.0",
    "angular-local-storage": "0.7.1",
    "angular-localforage": "1.3.7",
    "angular-material": "1.1.7",
    "angular-messages": "1.7.0",
    "angular-qrcode": "7.2.0",
    "angular-sanitize": "1.7.0",
    "angular-zxcvbn": "3.2.2",
    "async": "2.6.0",
    "babel-runtime": "^6.26.0",
    "bignumber.js": "4.1.0",
    "bluebird": "3.5.1",
    "crypto": "1.0.1",
    "decompress": "4.2.0",
    "electron": "1.7.12",
    "electron-compile": "6.4.2",
    "electron-log": "2.2.14",
    "electron-settings": "3.1.4",
    "electron-squirrel-startup": "1.0.0",
    "electron-updater": "2.17.6",
    "ethereumjs-tx": "1.3.3",
    "ethereumjs-util": "5.1.2",
    "fibers": "2.0.0",
    "fs-extra": "5.0.0",
    "google-charts": "1.0.0",
    "is-online": "^7.0.0",
    "keccak": "1.2.0",
    "keythereum": "1.0.2",
    "knex": "0.14.4",
    "ledger-wallet-provider": "2.0.1-beta.2",
    "mime-types": "2.1.17",
    "mv": "2.1.1",
    "qrcode-generator": "1.3.1",
    "request": "2.83.0",
    "secp256k1": "3.2.5",
    "sjcl": "1.0.6",
    "sqlite3": "3.1.13",
    "uuid": "3.0.0",
    "web3": "1.0.0-beta.33",
    "web3-provider-engine": "14.0.4",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "electron-forge": "4.1.7",
    "electron-prebuilt-compile": "1.7.9",
    "gulp": "3.9.1",
    "gulp-angular-templatecache": "2.0.0",
    "gulp-pug": "3.3.0",
    "gulp-rename": "1.2.2",
    "gulp-sass": "3.1.0",
    "gulp-token-replace": "1.0.4",
    "gulp-watch": "4.3.11",
    "node-notifier": "5.1.2",
    "node-sass": "4.6.1",
    "require-reload": "0.2.2",
    "run-sequence": "2.1.0",
    "vinyl-source-stream": "1.1.0",
    "chai": "^4.1.2",
    "chalk": "^2.3.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "delay": "^2.0.0",
    "electron": "1.7.11",
    "electron-builder": "^20.0.4",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "mocha": "^5.0.1",
    "prettier": "^1.10.2",
    "serial-mocha": "0.0.4",
    "spectron": "3.7.3"
  },
  "migrations": {
    "version": "20180508093508"
  },
  "seeds": {
    "version": "init"
  },
  "build": {
    "name": "selfkey-identity-wallet",
    "appId": "org.selfkey.wallet"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg"
        ],
        "linux": [
          "zip"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "name": "selfkey-identity-wallet",
        "productName": "SelfKey Identity Wallet",
        "overwrite": true,
        "asar": true,
        "icon": "assets/icons/selfkey",
        "ignore": [
          "gulp-tasks",
          "wallet-web-app"
        ]
      },
      "electronInstallerDMG": {
        "name": "SelfKey Identity Wallet",
        "icon": "./assets/icons/mac/selfkey.icns",
        "background": "./assets/backgrounds/mac/installer.jpg",
        "overwrite": true
      },
      "electronWinstallerConfig": {
        "name": "selfkey-identity-wallet",
        "title": "SelfKey Identity Wallet",
        "authors": "Selfkey Foundation"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "SelfKey Identity Wallet"
      }
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
