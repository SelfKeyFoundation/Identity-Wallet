	"seeds": {
		"version": "init"
	},
	"build": {
		"appId": "org.selfkey.wallet",
		"buildDependenciesFromSource": true,
		"generateUpdatesFilesForAllChannels": true,
		"afterSign": "scripts/notarize.js",
		"asar": true,
		"files": [
			"dist/main",
			"dist/common",
			"!**/stories/**"
		],
		"extraFiles": [
			{
				"from": "dist/common",
				"to": "./common"
			},
			{
				"from": "dist/main",
				"to": "./main"
			}
		],
		"mac": {
			"category": "public.app-category.finance",
			"target": [
				"dmg",
				"zip",
				"pkg"
			],
			"hardenedRuntime": true,
			"gatekeeperAssess": false,
			"entitlements": "build/entitlements.mac.plist",
			"entitlementsInherit": "build/entitlements.mac.plist"
		},
		"dmg": {
			"sign": false
		},
		"pkg": {
			"installLocation": "/Applications"
		},
		"linux": {
			"target": [
				{
					"target": "AppImage",
					"arch": [
						"x64",
						"armv7l"
					]
				},
				{
					"target": "tar.gz",
					"arch": [
						"armv7l"
					]
				}
			]
		},
		"protocols": [
			{
				"name": "Selfkey",
				"schemes": [
					"selfkey"
				]
			}
		],
		"win": {
			"target": "nsis"
		}
	},
	"electronWebpack": {
		"whiteListedModules": [
			"source-map-support",
			"react",
			"react-dom",
			"react-redux",
			"react-router-dom",
			"ajv",
			"react-jsonschema-form-material-theme",
