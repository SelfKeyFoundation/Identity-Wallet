			"redux-promise",
			"electron-dl",
			"electron-is",
			"lodash",
			"normalizr",
			"socket.io-client",
			"striptags",
			"trezor-wallet-provider",
			"web3-provider-engine",
			"prop-types",
			"moment",
			"bignumber.js",
			"connected-react-router",
			"@material-ui/core",
			"@material-ui/icons",
			"flag-icon-css/css/flag-icon.css",
			"fs-extra",
			"node-machine-id"
		],
		"main": {
			"extraEntries": [
				"@/preload.js"
			],
			"webpackConfig": "webpack/main.additions.js"
		},
		"renderer": {
			"dll": [
				"react",
				"react-dom",
				"react-redux",
				"react-router-dom",
				"ajv",
				"react-jsonschema-form",
				"redux",
				"redux-thunk",
				"reselect",
				"request-promise-native",
				"selfkey-ui",
				"redux-logger",
				"redux-promise",
				"electron-dl",
				"electron-is",
				"lodash",
				"normalizr",
				"socket.io-client",
				"striptags",
				"trezor-wallet-provider",
				"web3-provider-engine",
				"prop-types",
				"moment",
				"bignumber.js",
				"connected-react-router",
				"@material-ui/core",
				"@material-ui/icons",
				"flag-icon-css/css/flag-icon.css",
				"fs-extra",
				"node-machine-id",
				"./static/stylesheets/scss/main.scss"
			],
			"webpackConfig": "webpack/renderer.additions.js",
			"webpackDllConfig": "webpack/dll.additions.js"
		}
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"snyk": true
