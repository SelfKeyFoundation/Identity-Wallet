			"moment",
			"bignumber.js",
			"connected-react-router",
			"@material-ui/core",
			"@material-ui/icons",
			"@material-ui/styles",
			"@material-ui/lab",
			"flag-icon-css/css/flag-icon.css",
			"fs-extra",
			"node-machine-id"
		],
		"main": {
			"extraEntries": [
				"@/preload.js"
			],
			"webpackConfig": "webpack/main.additions.js"
		},
		"renderer": {
			"dll": [
				"react",
				"react-dom",
				"react-redux",
				"react-router-dom",
				"ajv",
				"react-jsonschema-form-material-theme",
				"redux",
				"redux-thunk",
				"reselect",
				"request-promise-native",
				"selfkey-ui",
				"redux-logger",
				"redux-promise",
				"electron-dl",
				"electron-is",
				"lodash",
				"normalizr",
