{
  "name": "selfkey-identity-wallet",
  "productName": "SelfKey Identity Wallet",
  "version": "0.0.23",
  "description": "Selfkey Identity Wallet",
  "repository": "https://github.com/SelfKeyFoundation/Identity-Wallet",
  "main": "./app/main.js",
  "scripts": {
    "start": "gulp && ./node_modules/.bin/electron-forge start -- dev",
    "prod": "gulp && ./node_modules/.bin/electron-forge start -- prod",
    "package": "gulp && ./node_modules/.bin/electron-forge package",
    "make": "gulp && ./node_modules/.bin/electron-forge make",
    "makewin": "gulp && node_modules\\.bin\\electron-forge make",
    "startwin": "gulp && node_modules\\.bin\\electron-forge start -- dev",
    "start-auto": "gulp && gulp start",
    "make-auto": "gulp && gulp make"
  },
  "dependencies": {
    "@uirouter/angularjs": "1.0.6",
    "angular": "1.6.7",
    "angular-animate": "1.6.7",
    "angular-aria": "1.6.7",
    "angular-local-storage": "0.7.1",
    "angular-localforage": "1.3.7",
    "angular-material": "1.1.7",
    "angular-messages": "1.6.7",
    "angular-qrcode": "7.2.0",
    "angular-zxcvbn": "3.2.2",
    "async": "2.6.0",
    "bignumber.js": "4.1.0",
    "bluebird": "3.5.1",
    "camelcase": "4.1.0",
    "crypto": "1.0.1",
    "decompress": "4.2.0",
    "deskmetrics": "2.3.0-rc7",
    "electron": "1.7.11",
    "electron-compile": "6.4.2",
    "electron-log": "2.2.14",
    "electron-pdf-window": "1.0.12",
    "electron-settings": "3.1.4",
    "electron-squirrel-startup": "1.0.0",
    "electron-updater": "2.17.6",
    "ethereumjs-tx": "1.3.3",
    "ethereumjs-util": "5.1.2",
    "fibers": "2.0.0",
    "fs-extra": "5.0.0",
    "google-charts": "1.0.0",
    "jquery": "3.2.1",
    "keccak": "1.2.0",
    "keythereum": "1.0.2",
    "knex": "0.14.4",
    "lodash": "4.17.4",
    "mime-types": "2.1.17",
    "mv": "2.1.1",
    "node-json-rpc": "0.0.1",
    "qrcode-generator": "1.3.1",
    "request": "^2.83.0",
    "secp256k1": "3.2.5",
    "sjcl": "1.0.6",
    "sqlite3": "^3.1.13",
    "uuid": "3.0.0",
    "web3": "1.0.0-beta.30",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "electron-forge": "4.1.7",
    "electron-prebuilt-compile": "1.7.9",
    "gulp": "3.9.1",
    "gulp-angular-templatecache": "2.0.0",
    "gulp-pug": "3.3.0",
    "gulp-rename": "1.2.2",
    "gulp-sass": "3.1.0",
    "gulp-token-replace": "1.0.4",
    "gulp-watch": "4.3.11",
    "node-notifier": "5.1.2",
    "node-sass": "4.6.1",
    "require-reload": "0.2.2",
    "run-sequence": "2.1.0",
    "vinyl-source-stream": "1.1.0"
  },
  "build": {
    "name": "selfkey-identity-wallet",
    "appId": "org.selfkey.wallet"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg"
        ],
        "linux": [
          "zip"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "name": "selfkey-identity-wallet",
        "productName": "SelfKey Identity Wallet",
        "overwrite": true,
        "asar": true,
        "icon": "assets/icons/selfkey",
        "ignore": [
          "gulp-tasks",
          "wallet-web-app"
        ]
      },
      "electronInstallerDMG": {
        "name": "SelfKey Identity Wallet",
        "icon": "./assets/icons/mac/selfkey.icns",
        "background": "./assets/backgrounds/mac/installer.jpg",
        "overwrite": true
      },
      "electronWinstallerConfig": {
        "name": "selfkey-identity-wallet",
        "title": "SelfKey Identity Wallet",
        "authors": "Selfkey Foundation"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "SelfKey Identity Wallet"
      }
    }
  }
}
