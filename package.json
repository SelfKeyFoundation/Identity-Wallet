{
  "name": "selfkey-identity-wallet",
  "productName": "SelfKey Identity Wallet",
  "version": "0.0.7-beta",
  "description": "The Official SelfKey Identity Wallet for Desktop",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SelfKeyFoundation/Identity-Wallet.git"
  },
  "keywords": [
    "selfkey",
    "identity",
    "wallet",
    "cryptocurrency",
    "ethereum",
    "erc20"
  ],
  "author": {
    "name": "SelfKey Foundation",
    "email": "info@selfkey.org",
    "url": "https://selfkey.org/"
  },
  "bugs": {
    "url": "https://github.com/SelfKeyFoundation/Identity-Wallet/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/MIT"
    }
  ],
  "main": "./src/main/main.js",
  "scripts": {
    "dev": "gulp && electron-webpack dev",
    "install-app-deps": "electron-builder install-app-deps",
    "compile": "gulp && electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "test": "node test/test.js",
    "publish-build": "yarn compile && electron-builder -p always",
    "precommit": "pretty-quick --staged",
    "commitmsg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@uirouter/angularjs": "1.0.18",
    "angular": "1.7.0",
    "angular-animate": "1.7.0",
    "angular-aria": "1.7.0",
    "angular-local-storage": "0.7.1",
    "angular-localforage": "1.3.8",
    "angular-material": "1.1.9",
    "angular-messages": "1.7.0",
    "angular-qrcode": "7.2.0",
    "angular-sanitize": "1.7.0",
    "angular-zxcvbn": "3.2.2",
    "async": "2.6.1",
    "bignumber.js": "7.2.1",
    "bluebird": "3.5.1",
    "crypto": "1.0.1",
    "decompress": "4.2.0",
    "ethereumjs-tx": "1.3.4",
    "ethereumjs-util": "5.2.0",
    "fibers": "2.0.2",
    "fs-extra": "6.0.1",
    "google-charts": "1.1.0",
    "is-online": "^7.0.0",
    "keccak": "1.4.0",
    "keythereum": "1.0.4",
    "knex": "^0.14.6",
    "ledgerco": "1.2.1",
    "mime-types": "2.1.18",
    "mv": "2.1.1",
    "promise-timeout": "1.3.0",
    "qrcode-generator": "1.4.0",
    "request": "2.87.0",
    "secp256k1": "3.5.0",
    "sjcl": "1.0.7",
    "source-map-support": "^0.5.6",
    "sqlite3": "^4.0.0",
    "striptags": "^3.1.1",
    "uuid": "3.2.1",
    "web3": "1.0.0-beta.34",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "chai": "^4.1.2",
    "chalk": "2.3.2",
    "copy-webpack-plugin": "^4.5.1",
    "cz-conventional-changelog": "^2.1.0",
    "delay": "^3.0.0",
    "electron": "2.0.2",
    "electron-builder": "20.14.7",
    "electron-chromedriver": "^2.0.0",
    "electron-log": "^2.2.14",
    "electron-settings": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^2.21.10",
    "electron-webpack": "^2.1.2",
    "electron-webpack-eslint": "^2.1.1",
    "gulp": "^3.9.1",
    "gulp-angular-templatecache": "^2.2.1",
    "gulp-pug": "^4.0.1",
    "gulp-sass": "^4.0.1",
    "gulp-watch": "^5.0.0",
    "husky": "^0.14.3",
    "json-loader": "^0.5.7",
    "mocha": "^5.2.0",
    "pretty-quick": "^1.6.0",
    "run-sequence": "^2.2.1",
    "spectron": "^3.8.0",
    "webpack": "^4.8.1",
    "webpack-build-notifier": "^0.1.25",
    "webpack-sources": "1.1.0"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  },
  "migrations": {
    "version": "20180508093508"
  },
  "seeds": {
    "version": "init"
  },
  "build": {
    "appId": "org.selfkey.wallet",
    "generateUpdatesFilesForAllChannels": true,
    "files": [
      "dist/migrations",
      "dist/seeds",
      "dist/assets"
    ]
  },
  "electronWebpack": {
    "whiteListedModules": [
      "source-map-support"
    ],
    "main": {
      "extraEntries": [
        "@/preload.js"
      ],
      "webpackConfig": "webpack.main.additions.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
