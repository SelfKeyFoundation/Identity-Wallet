{
	"name": "Selfkey-Identity-Wallet",
	"productName": "Selfkey-Identity-Wallet",
	"version": "0.0.9",
	"description": "Identity Wallet",
	"repository": "https://github.com/kyc-chain/wallet",
	"main": "./app/main.js",
	"scripts": {
		"start": "gulp && ./node_modules/.bin/electron-forge start -- dev",
		"package": "gulp && ./node_modules/.bin/electron-forge package",
		"make": "gulp && ./node_modules/.bin/electron-forge make"
	},
	"dependencies": {
		"@uirouter/angularjs": "1.0.6",
		"angular": "1.5.8",
		"angular-animate": "1.5.8",
		"angular-aria": "1.5.8",
		"angular-local-storage": "0.7.1",
		"angular-localforage": "1.3.7",
		"angular-material": "1.1.4",
		"angular-messages": "1.5.8",
		"angular-qrcode": "7.2.0",
		"async": "2.6.0",
		"bignumber.js": "4.1.0",
		"camelcase": "4.1.0",
		"crypto": "1.0.1",
		"decompress": "4.2.0",
		"deskmetrics": "2.3.0-rc7",
		"electron": "1.7.5",
		"electron-compile": "6.4.2",
		"electron-settings": "3.1.4",
		"electron-squirrel-startup": "1.0.0",
		"electron-updater": "2.17.6",
		"ethereumjs-tx": "1.3.3",
		"ethereumjs-util": "5.1.2",
		"fibers": "2.0.0",
		"fs-extra": "5.0.0",
		"google-charts": "1.0.0",
		"jquery": "3.2.1",
		"keccak": "1.2.0",
		"keythereum": "1.0.2",
		"lodash": "4.17.4",
		"mime-types": "2.1.17",
		"mv": "2.1.1",
		"node-json-rpc": "0.0.1",
		"secp256k1": "3.2.5",
		"sjcl": "1.0.6",
		"uuid": "3.0.0",
		"web3": "1.0.0-beta.26"
	},
	"devDependencies": {
		"babel-plugin-transform-async-to-generator": "6.24.1",
		"babel-preset-env": "1.6.1",
		"babel-preset-es2015": "6.24.1",
		"electron-forge": "4.1.7",
		"electron-prebuilt-compile": "1.7.9",
		"gulp": "3.9.1",
		"gulp-angular-templatecache": "2.0.0",
		"gulp-pug": "3.3.0",
		"gulp-rename": "1.2.2",
		"gulp-sass": "3.1.0",
		"gulp-token-replace": "1.0.4",
		"gulp-watch": "4.3.11",
		"node-notifier": "5.1.2",
		"node-sass": "4.6.1",
		"require-reload": "0.2.2",
		"run-sequence": "2.1.0",
		"vinyl-source-stream": "1.1.0"
	},
	"build": {
		"appId": "org.selfkey.wallet"
	},
	"config": {
		"forge": {
			"make_targets": {
				"win32": [
					"squirrel"
				],
				"darwin": [
					"dmg"
				],
				"linux": [
					"zip"
				]
			},
			"electronPackagerConfig": {
				"packageManager": "npm",
				"name": "Selfkey-Identity-Wallet",
				"productName": "Selfkey-Identity-Wallet",
				"overwrite": true,
				"asar": true,
				"ignore": [
					"gulp-tasks",
					"wallet-web-app"
				]
			},
			"electronInstallerDMG": {
				"name": "Selfkey-Identity-Wallet",
				"icon": "./assets/icons/mac/selfkey.icns",
				"background": "./assets/backgrounds/mac/installer.jpg",
				"overwrite": true
			},
			"electronWinstallerConfig": {
				"name": "Selfkey-Identity-Wallet",
				"title": "Selfkey Identity Wallet",
				"authors": "Selfkey Foundation"
			},
			"electronInstallerDebian": {},
			"electronInstallerRedhat": {},
			"github_repository": {
				"owner": "",
				"name": ""
			},
			"windowsStoreConfig": {
				"packageName": "",
				"name": "identitywallet"
			}
		}
	}
}
