{
  "name": "selfkey-identity-wallet",
  "productName": "SelfKey Identity Wallet",
  "version": "0.0.1",
  "description": "The Official SelfKey Identity Wallet for Desktop",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SelfKeyFoundation/Identity-Wallet.git"
  },
  "keywords": [
    "selfkey",
    "identity",
    "wallet",
    "cryptocurrency",
    "ethereum",
    "erc20"
  ],
  "author": {
    "name": "SelfKey Foundation",
    "email": "info@selfkey.org",
    "url": "https://selfkey.org/"
  },
  "bugs": {
    "url": "https://github.com/SelfKeyFoundation/Identity-Wallet/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/MIT"
    }
  ],
  "main": "./app/main.js",
  "scripts": {
    "dev": "gulp && electron-webpack dev",
    "postinstall": "ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true electron-builder install-app-deps",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "test": "node test/test.js"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@uirouter/angularjs": "1.0.18",
    "angular": "1.7.0",
    "angular-animate": "1.7.0",
    "angular-aria": "1.7.0",
    "angular-local-storage": "0.7.1",
    "angular-localforage": "1.3.8",
    "angular-material": "1.1.9",
    "angular-messages": "1.7.0",
    "angular-qrcode": "7.2.0",
    "angular-sanitize": "1.7.0",
    "angular-zxcvbn": "3.2.2",
    "async": "2.6.1",
    "bignumber.js": "7.2.1",
    "bluebird": "3.5.1",
    "crypto": "1.0.1",
    "decompress": "4.2.0",
    "ethereumjs-tx": "1.3.4",
    "ethereumjs-util": "5.2.0",
    "fibers": "2.0.2",
    "fs-extra": "6.0.1",
    "google-charts": "1.1.0",
    "is-online": "^7.0.0",
    "keccak": "1.4.0",
    "keythereum": "1.0.4",
    "knex": "^0.14.6",
    "ledger-wallet-provider": "2.0.1-beta.2",
    "mime-types": "2.1.18",
    "mv": "2.1.1",
    "qrcode-generator": "1.4.0",
    "request": "2.87.0",
    "secp256k1": "3.5.0",
    "sjcl": "1.0.7",
    "source-map-support": "^0.5.6",
    "sqlite3": "^4.0.0",
    "uuid": "3.2.1",
    "web3": "1.0.0-beta.34",
    "web3-provider-engine": "14.0.5",
    "webpack-sources": "1.1.0",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "delay": "^3.0.0",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "electron-log": "^2.2.14",
    "electron-settings": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-webpack": "^2.1.2",
    "electron-webpack-eslint": "^2.1.1",
    "gulp": "^3.9.1",
    "gulp-angular-templatecache": "^2.2.1",
    "gulp-pug": "^4.0.1",
    "gulp-sass": "^4.0.1",
    "gulp-watch": "^5.0.0",
    "mocha": "^5.2.0",
    "run-sequence": "^2.2.1",
    "spectron": "^3.8.0",
    "webpack": "^4.8.1",
    "webpack-build-notifier": "^0.1.25"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  },
  "migrations": {
    "version": "20180508093508"
  },
  "seeds": {
    "version": "init"
  },
  "build": {
    "appId": "org.selfkey.wallet"
  },
  "electronWebpack": {
    "whiteListedModules": [
      "source-map-support"
    ],
    "main": {
      "extraEntries": [
        "@/preload.js"
      ]
    }
  }
